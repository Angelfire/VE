{
  "arrowParens": "avoid",
  "endOfLine": "lf",
  "printWidth": 100,
  "semi": false,
  "singleQuote": false,
  "tabWidth": 2,
  "trailingComma": "es5",
  "useTabs": false,
  "plugins": ["prettier-plugin-astro", "prettier-plugin-tailwindcss"],
  "pluginSearchDirs": false,
  "overrides": [
    {
      "files": "**/*astro",
      "options": {
        "parser": "astro"
      }
    }
  ]
}
